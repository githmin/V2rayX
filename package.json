{
  "name": "v2rayx",
  "version": "0.4.0",
  "description": "An all-platform (Macos Windows Linux) V2ray client build with electron.",
  "main": "./out/main/index.js",
  "author": "shaonhuang",
  "homepage": "https://github.com/shaonhuang/V2rayX",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "publish:local": "electron-vite build && electron-builder",
    "publish:win": "npm run build:win && electron-builder --win --publish always",
    "publish:mac": "npm run build:mac && electron-builder --mac --publish always",
    "publish:linux": "npm run build:linux && electron-builder --linux --publish always",
    "publish:all": "npm run build && electron-builder --win --mac --linux --publish always",
    "wdio": "wdio run ./wdio.conf.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/system": "^5.15.2",
    "@mui/x-data-grid": "^6.18.6",
    "@mui/x-date-pickers": "^6.18.6",
    "@sentry/electron": "^4.16.0",
    "@tanstack/react-query": "^5.17.0",
    "adm-zip": "^0.5.10",
    "animate.css": "^4.1.1",
    "buffer": "^6.0.3",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "compare-versions": "^6.1.0",
    "cron": "^3.1.6",
    "desktop-screenshot": "^0.1.1",
    "electron-progressbar": "^2.1.0",
    "electron-updater": "6.1.7",
    "eventemitter3": "^5.0.1",
    "jsqr": "^1.4.0",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "material-react-table": "^2.3.1",
    "monaco-editor": "^0.45.0",
    "net": "^1.0.2",
    "object-hash": "^3.0.0",
    "qrcode": "^1.5.3",
    "react-router-dom": "^6.21.1",
    "redux-logger": "^3.0.6",
    "rxjs": "^7.8.1",
    "tapable": "^2.2.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.2.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/adm-zip": "^0.5.5",
    "@types/electron-progressbar": "^1.2.6",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^9.0.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.17.14",
    "@types/object-hash": "^3.0.6",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.12",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@wdio/cli": "^8.27.1",
    "@wdio/local-runner": "^8.27.0",
    "@wdio/mocha-framework": "^8.27.0",
    "@wdio/spec-reporter": "^8.27.0",
    "@wdio/types": "^8.27.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "24.10.0",
    "electron-vite": "^2.0.0-beta.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "html-to-text": "^9.0.5",
    "js-base64": "^3.7.5",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "wdio-electron-service": "^6.0.2",
    "webdriverio": "^8.27.0"
  },
  "packageManager": "yarn@4.0.2"
}
