{
  "name": "v2rayx",
  "version": "0.1.2",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "shaonhuang",
  "homepage": "https://github.com/shaonhuang/V2rayX",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "publish:local": "electron-vite build && electron-builder",
    "publish:win": "npm run build:win && electron-builder --win --publish always",
    "publish:mac": "npm run build:mac && electron-builder --mac --publish always",
    "publish:linux": "npm run build:linux && electron-builder --linux --publish always",
    "publish:all": "npm run build && electron-builder --win --mac --linux --publish always"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@types/md5": "^2.3.2",
    "@types/object-hash": "^3.0.2",
    "adm-zip": "^0.5.10",
    "animate.css": "^4.1.1",
    "chalk": "^4",
    "electron-is-dev": "^2.0.0",
    "electron-progressbar": "^2.1.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "jsqr": "^1.4.0",
    "load-json-file": "^7.0.1",
    "lodash": "^4.17.21",
    "lowdb": "^6.0.1",
    "object-hash": "^3.0.0",
    "open": "7.0.3",
    "react-json-view": "^1.21.3",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "@mui/styled-engine-sc": "^5.11.0",
    "@reduxjs/toolkit": "^1.9.4",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "@types/superagent": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.21",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "js-base64": "^3.7.5",
    "md5": "^2.3.0",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux-thunk": "^2.4.2",
    "superagent": "^8.0.9",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  },
  "packageManager": "yarn@3.5.0"
}
